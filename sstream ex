
#include <iostream>
#include <sstream>
#include <string>
using namespace std;

int main() {
   string objectInfo = "Headphones 10 11";
   istringstream objectISS(objectInfo);
   string object;
   int quantity;
   int price;
   
   objectISS >> object >> price >> quantity;
   
   cout << object << " x" << quantity << endl;
   cout << "Price: " << price;
   
   return 0;
}

///////* output
Headphones x11
Price: 10 *////////
